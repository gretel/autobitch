set -e

### ...


### automagic
use_auto() {
  # prevent one line of verbose logging
  export DIRENV_LOG_FORMAT=""

  ### include logic
  source_env "$HOME/.automagic"

  ### decorate log output
  log_prefix "$(expand_path "$1")"
}
